$(function () {
    var playNote = function (files) {
        if (this.fileIndex + 1 >= files.length) {
            this.removeEventListener('ended', playNote);
            this.fileIndex = 0;
            this.src = "https://discoverygc.com/fldialogue/" + files[0] + ".wav";
        } else {
            this.src = "https://discoverygc.com/fldialogue/" + files[++this.fileIndex] + ".wav";
            this.play();
        }
    };

    Array.from(document.getElementsByClassName('fldialogueplayer')).forEach(function (player) {
        var files = player.dataset.audioseq.split(',');
        player.fileIndex = 0;
        player.src = "https://discoverygc.com/fldialogue/" + files[0] + ".wav";
        if (files.length <= 8) { // no sequence of audio files go on more than 8 files
            player.addEventListener('ended', playNote.bind(player, files));
        }
    });
